<theme>
    <formatVersion>4</formatVersion>
	<include subset="colorset" name="Full HD 1080p">./colorsets/fullhd.xml</include>
	<include subset="colorset" name="HD 720p">./colorsets/hd.xml</include>
	<include subset="colorset" name="Lite 540p">./colorsets/lite.xml</include>
	
	<include subset="iconset" name="standard icons" displayName="${icondef}">./iconsets/default.xml</include>
	<include subset="iconset" name="SNES">./iconsets/snes.xml</include>
	<include subset="iconset" name="SNES ALT">./iconsets/snesalt.xml</include>
	<include subset="iconset" name="XBox">./iconsets/xbox.xml</include>
	<include subset="iconset" name="Default PSX">./iconsets/psx.xml</include>
	<include subset="iconset" name="ARCADE">./iconsets/arcade.xml</include>
