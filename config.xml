	<include subset="systemview" name="Random Video Background" displayName="${svrandvid}">./systemview/7AA.xml</include>
	<include subset="systemview" name="Dynamic Background" displayName="${svwheel}">./systemview/7A.xml</include>
	<include subset="systemview" name="Custom Background Image" displayName="${scustimg}">./systemview/7AD.xml</include>
	
